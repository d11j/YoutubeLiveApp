{
  "name": "youtube_live",
  "version": "0.2.2",
  "author": "happou31",
  "description": "app for youtube live console.",
  "main": "index.js",
  "engines": {
    "node": "12.13.x"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development electron ./dist",
    "upgrade-minor": "yarn upgrade -L --tilde",
    "format": "eslint --fix ./src/**/*.{ts,tsx}",
    "test": "mocha -r tsconfig-paths/register -r ts-node/register tests/**/*.test.ts",
    "dev": "cross-env NODE_ENV=development webpack --watch --config ./webpack.config.js --mode development",
    "make:page": "node ./bin/page.js",
    "build:win": "echo This command is only for Windows CommandPrompt. && del /S /Q dist\\* && cross-env NODE_ENV=production webpack --config ./webpack.config.js --mode production && copy package.json dist\\ && electron-packager ./dist YoutubeLiveApp --platform=win32 --arch=x64 --icon=./images/icon.ico --electron-version=9.0.4 --overwrite && pushd YoutubeLiveApp-win32-x64"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "npm run format",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/copy-paste": "^1.1.30",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.13",
    "@types/react": "~16.9.38",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/styled-components": "^5.1.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^6.0.1",
    "cross-env": "^7.0.2",
    "electron": "^9.0.4",
    "electron-packager": "^14.2.1",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "lint-staged": "~10.2.11",
    "marked": "^1.1.1",
    "mocha": "^8.0.1",
    "prettier": "^2.0.5",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.9.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "copy-paste": "https://github.com/PKief/node-copy-paste.git#adjust-platform-require",
    "electron-context-menu": "^2.3.0",
    "electron-redux": "^1.5.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.1",
    "styled-components": "^5.1.1",
    "uuid": "^8.3.0"
  },
  "license": "MIT"
}
